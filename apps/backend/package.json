  "version": "1.0.0",
  "main": "index.js",
  "requires": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:prod": "NODE_ENV=production node dist/src/index.js",
    "generate": "cd ../../packages/db && npx prisma generate",
    "migrate": "cd ../../packages/db && npx prisma migrate dev",
    "postinstall": "npm run generate",
    "test:redis": "node scripts/test-redis.js",
    "test:upstash": "node scripts/test-redis.js",
    "cron:status": "curl -X GET http://localhost:3001/api/cron/status",
    "cron:run": "curl -X POST http://localhost:3001/api/cron/run",
    "health": "curl -X GET http://localhost:3001/healthz"
  },
